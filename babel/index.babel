    )
  }
}

class PupilRight extends React.Component {
  render() {
    return (
      <div className="pupil-right"></div>
    )
  }
}

class Saying extends React.Component {
  render (){
    return(
      <div className="saying">
        <h4>"In this world that we live in, a koala is all you need. I need one too," says the Koala, in despair.</h4>
      </div>
    )
  }
}


ReactDOM.render(
  <Koala/>,
  document.getElementById('koala')
);
